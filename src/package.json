{
  "name": "nativescript-fresco",
  "version": "3.0.6",
  "description": "This is a NativeScript plugin wrapping the popular Fresco library for managing images on Android.",
  "main": "nativescript-fresco",
  "repository": {
    "type": "git",
    "url": "https://github.com/NativeScript/nativescript-fresco"
  },
  "bugs": {
    "url": "https://github.com/NativeScript/nativescript-fresco/issues"
  },
  "files": [
    "*.js*",
    "platforms",
    "*.d.ts",
    "angular/*.js*",
    "angular/*.d.ts",
    "!tsconfig.json"
  ],
  "nativescript": {
    "platforms": {
      "android": "3.0.0",
      "ios": "3.0.0"
    }
  },
  "devDependencies": {
    "@angular/core": "4.2.5",
    "@angular/common": "4.2.5",
    "@angular/forms": "4.2.5",
    "@angular/http": "4.2.5",
    "@angular/platform-browser": "4.2.5",
    "@angular/compiler": "4.2.5",
    "@angular/compiler-cli": "4.2.5",
    "@angular/router": "4.2.5",
    "nativescript-angular": "4.2.0",
    "rxjs": "^5.0.1",
    "tns-core-modules": "3.1.1",
    "tns-platform-declarations": "3.1.1",
    "typescript": "^2.2.2",
    "zone.js": "0.8.4"
  },
  "peerDependencies": {
    "tns-core-modules": "^3.1.0 || >3.3.0-"
  },
  "scripts": {
    "prepack": "ngc",
    "tsc": "tsc -skipLibCheck"
  },
  "keywords": [
    "fresco",
    "android",
    "nativescript"
  ],
  "author": "NativeScript Team",
  "license": "Apache-2.0"
}
